{
    "name": "shaoyikai/api-doc",
    "description": "An excellent api document generator",
    "authors": [
        {
            "name": "shaoyikai",
            "email": "shaoyikai@qq.com"
        }
    ],

    "keywords": ["yii2", "api doc", "", "project template"],
    "homepage": "https://github.com/shaoyikai/api-doc",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/shaoyikai/api-doc/issues",
        "source": "https://github.com/shaoyikai/api-doc"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.5",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "phpdocumentor/phpdocumentor": "2.*",
        "cliff363825/yii2-kindeditor": "v1.0.5"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}